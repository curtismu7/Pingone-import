{
  "name": "pingone-import",
  "version": "1.1.2",
  "description": "A modern web application for importing users into PingOne using the PingOne Admin API.",
  "main": "server.cjs",
  "scripts": {
    "start": "node server.cjs",
    "dev": "nodemon server.cjs",
    "build": "browserify public/js/app.js -o public/js/bundle.js",
    "build:prod": "browserify public/js/app.js | uglifyjs -c -m > public/js/bundle.min.js",
    "watch": "watchify public/js/app.js -o public/js/bundle.js --debug --verbose",
    "test": "jest",
    "test:unit": "jest test/ui-manager.test.js",
    "test:integration": "node test/logging-test.js",
    "test:api": "jest test/integration/pingone-api.test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "NODE_ENV=test jest --ci --runInBand",
    "test:all": "jest",
    "setup": "cp .env.example .env"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "rotating-file-stream": "^3.2.6",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/runtime": "^7.27.6",
    "@jest/globals": "^29.7.0",
    "babel-jest": "^29.7.0",
    "browserify": "^17.0.1",
    "chai": "^5.2.0",
    "core-js": "^3.43.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "mocha": "^11.7.1",
    "nodemon": "^2.0.22",
    "uglify-js": "^3.19.3",
    "watchify": "^4.0.0",
    "whatwg-url": "^14.2.0"
  }
}
